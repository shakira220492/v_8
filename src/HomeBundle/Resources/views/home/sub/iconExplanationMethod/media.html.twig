<script>
    var mediaIconButton = document.getElementById('mediaIconButton');

    mediaIconButton.addEventListener("mouseover", function(){mouseover(fieldName="mediaIconContent", event="mouseover");});
    mediaIconButton.addEventListener("mouseout", function(){mouseout(fieldName="mediaIconContent", event="mouseout");});
    mediaIconButton.addEventListener("click", function(){click(fieldName="mediaIconContent", event="click");});
</script>