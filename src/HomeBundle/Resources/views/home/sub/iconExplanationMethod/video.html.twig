<script>
    var videoIconButton = document.getElementById('videoIconButton');
    
    videoIconButton.addEventListener("mouseover", function(){mouseover(fieldName="videoIconContent", event="mouseover");});
    videoIconButton.addEventListener("mouseout", function(){mouseout(fieldName="videoIconContent", event="mouseout");});
    videoIconButton.addEventListener("click", function(){click(fieldName="videoIconContent", event="click");});
</script>