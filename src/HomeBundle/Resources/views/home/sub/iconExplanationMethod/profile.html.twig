<script>
    var profileIconButton = document.getElementById('profileIconButton');

    profileIconButton.addEventListener("mouseover", function(){mouseover(fieldName="profileIconContent", event="mouseover");});
    profileIconButton.addEventListener("mouseout", function(){mouseout(fieldName="profileIconContent", event="mouseout");});
    profileIconButton.addEventListener("click", function(){click(fieldName="profileIconContent", event="click");});
</script>