<script>
    var searchIconButton = document.getElementById('searchIconButton');
    
    searchIconButton.addEventListener("mouseover", function(){mouseover(fieldName="searchIconContent", event="mouseover");});
    searchIconButton.addEventListener("mouseout", function(){mouseout(fieldName="searchIconContent", event="mouseout");});
    searchIconButton.addEventListener("click", function(){click(fieldName="searchIconContent", event="click");});
</script>