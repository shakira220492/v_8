<script>
    var configurationIconButton = document.getElementById('configurationIconButton');

    configurationIconButton.addEventListener("mouseover", function(){mouseover(fieldName="configurationIconContent", event="mouseover");});
    configurationIconButton.addEventListener("mouseout", function(){mouseout(fieldName="configurationIconContent", event="mouseout");});
    configurationIconButton.addEventListener("click", function(){click(fieldName="configurationIconContent", event="click");});
</script>