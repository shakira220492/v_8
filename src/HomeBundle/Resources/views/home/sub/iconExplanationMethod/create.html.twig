<script>
    var createIconButton = document.getElementById('createIconButton');

    createIconButton.addEventListener("mouseover", function(){mouseover(fieldName="createIconContent", event="mouseover");});
    createIconButton.addEventListener("mouseout", function(){mouseout(fieldName="createIconContent", event="mouseout");});
    createIconButton.addEventListener("click", function(){click(fieldName="createIconContent", event="click");});
</script>