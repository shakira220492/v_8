<script>
    var listIconButton = document.getElementById('listIconButton');

    listIconButton.addEventListener("mouseover", function(){mouseover(fieldName="listIconContent", event="mouseover");});
    listIconButton.addEventListener("mouseout", function(){mouseout(fieldName="listIconContent", event="mouseout");});
    listIconButton.addEventListener("click", function(){click(fieldName="listIconContent", event="click");});
</script>